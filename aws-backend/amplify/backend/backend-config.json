{
    "storage": {
        "fellowbookdb": {
            "service": "DynamoDB",
            "providerPlugin": "awscloudformation"
        }
    },
    "function": {
        "fellowsEndpoint": {
            "build": true,
            "providerPlugin": "awscloudformation",
            "service": "Lambda",
            "dependsOn": [
                {
                    "category": "storage",
                    "resourceName": "fellowbookdb",
                    "attributes": [
                        "Name",
                        "Arn"
                    ],
                    "attributeEnvMap": {
                        "Name": "TABLE_NAME",
                        "Arn": "TABLE_ARN"
                    }
                }
            ]
        },
        "githubauth": {
            "build": true,
            "providerPlugin": "awscloudformation",
            "service": "Lambda"
        },
        "getaccesstoken": {
            "build": true,
            "providerPlugin": "awscloudformation",
            "service": "Lambda"
        }
    },
    "api": {
        "fellowbookapi": {
            "service": "API Gateway",
            "providerPlugin": "awscloudformation",
            "dependsOn": [
                {
                    "category": "function",
                    "resourceName": "fellowsEndpoint",
                    "attributes": [
                        "Name",
                        "Arn"
                    ]
                },
                {
                    "category": "function",
                    "resourceName": "getaccesstoken",
                    "attributes": [
                        "Name",
                        "Arn"
                    ]
                }
            ]
        }
    }
}