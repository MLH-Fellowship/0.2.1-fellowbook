{
    "storage": {
        "fellowsdb": {
            "service": "DynamoDB",
            "providerPlugin": "awscloudformation"
        }
    },
    "function": {
        "fellowsendpoint": {
            "build": true,
            "providerPlugin": "awscloudformation",
            "service": "Lambda",
            "dependsOn": [
                {
                    "category": "storage",
                    "resourceName": "fellowsdb",
                    "attributes": [
                        "Name",
                        "Arn"
                    ],
                    "attributeEnvMap": {
                        "Name": "TABLE_NAME",
                        "Arn": "TABLE_ARN"
                    }
                }
            ]
        },
        "fellowendpoint": {
            "build": true,
            "providerPlugin": "awscloudformation",
            "service": "Lambda",
            "dependsOn": [
                {
                    "category": "storage",
                    "resourceName": "fellowsdb",
                    "attributes": [
                        "Name",
                        "Arn"
                    ],
                    "attributeEnvMap": {
                        "Name": "TABLE_NAME",
                        "Arn": "TABLE_ARN"
                    }
                }
            ]
        },
        "githuborgauth": {
            "build": true,
            "providerPlugin": "awscloudformation",
            "service": "Lambda"
        },
        "githubauthtoken": {
            "build": true,
            "providerPlugin": "awscloudformation",
            "service": "Lambda"
        }
    },
    "auth": {},
    "api": {
        "fellowbookapi": {
            "service": "API Gateway",
            "providerPlugin": "awscloudformation",
            "dependsOn": [
                {
                    "category": "function",
                    "resourceName": "fellowsendpoint",
                    "attributes": [
                        "Name",
                        "Arn"
                    ]
                },
                {
                    "category": "function",
                    "resourceName": "fellowendpoint",
                    "attributes": [
                        "Name",
                        "Arn"
                    ]
                },
                {
                    "category": "function",
                    "resourceName": "githubauthtoken",
                    "attributes": [
                        "Name",
                        "Arn"
                    ]
                }
            ]
        }
    }
}